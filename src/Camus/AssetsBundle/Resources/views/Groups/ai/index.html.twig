{% elseif module.template == 'clubs' %}
  <div class="ai-index-clubs" >
    <div class="media-container">
      <span class="list-name" >{{ module.title }}</span>
      <ul class="list" >
        {% for module in module.modules if module != null %}
          {% set type = module.type|splitTemplate %}
          {#{% if module.id is defined and module.id != null %}
            {{ render_esi(controller('@CamusFrontEnd/Default/modulerender', {module: module.id })) }}
          {% else %}#}
            {% include '@CamusAssets/Groups/'~type[0]~'/'~type[1]~'.html.twig' ignore missing with {module: module|getModelModule} %}
          {#{% endif %}#}
        {% endfor %}
      </ul>
    </div>
